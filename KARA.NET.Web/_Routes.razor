<CascadingAuthenticationState>
    <Router AppAssembly="AssemblyUtils.Entry" AdditionalAssemblies="App.Assemblies">
        <Found Context="context">
            <AuthorizeRouteView RouteData="context" DefaultLayout="typeof(_Layout)" />
        </Found>
        <NotFound>
            <LayoutView Layout="typeof(_Layout)">
                <RadzenLabel Text="page not found" />
            </LayoutView>
        </NotFound>
    </Router>
</CascadingAuthenticationState>