@page "/password/list"
@attribute [Authorize]
<PageTitle>@Translator.GetTranslation(nameof(Translation.Passwords))</PageTitle>
<RadzenStack>
    <RadzenLabel Text="@Translator.GetTranslation(nameof(Translation.Passwords))" />
    @foreach (var password in this.Passwords)
    {
        <RadzenRow>
            <RadzenColumn Size="6">
                <RadzenLabel Text="@password.ID.ToString()" />
            </RadzenColumn>
            <RadzenColumn Size="6">
                <RadzenLabel Text="@password.Name" />
            </RadzenColumn>
        </RadzenRow>
    }
</RadzenStack>